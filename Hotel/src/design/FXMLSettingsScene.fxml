<AnchorPane id="AnchorPane" prefHeight="550.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="design.SettingsSceneController">
   <children>
      <JFXTextField fx:id="oldNameField" layoutX="330.0" layoutY="88.0" prefColumnCount="20" prefHeight="45.0" prefWidth="300.0" promptText="Old Name">
