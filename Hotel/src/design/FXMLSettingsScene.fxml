            <Font name="Calibri" size="20.0" />
         </font>
      </JFXPasswordField>
      <JFXPasswordField fx:id="newPassField" layoutX="330.0" layoutY="311.0" prefHeight="45.0" prefWidth="300.0" promptText="New Password">
