                <ImageView id="4" fx:id="settingsImage" fitHeight="40.0" fitWidth="40.0" onMouseEntered="#entered" onMouseExited="#exited" onMousePressed="#pressed" opacity="0.5" pickOnBounds="true" preserveRatio="true">
                    <image>
                        <Image url="@settings_100px.png" />
