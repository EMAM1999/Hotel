                    <image>
                        <Image url="@back_48px.png" />
                    </image>
                    <cursor>
                    </cursor>
                </ImageView>
                <ImageView id="1" fx:id="receptionImage" fitHeight="40.0" fitWidth="40.0" onMouseEntered="#entered" onMouseExited="#exited" onMousePressed="#pressed" opacity="0.5" pickOnBounds="true" preserveRatio="true">
